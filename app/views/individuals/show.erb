<h2>Welcome <%=current_user.username%>!</h2>
  <h3><%=@tree.name%></h3>
    <h4><%=@individual.name%>'s Relationships:</h4>
      
        <%if @individual.run_gcif? %>
          <%@individual.gender_children_if_present.each do |child|%>
             <ul>Child: <a href="/family_trees/<%=@tree.id%>/individuals/<%=child.id%>"><%=child.name%></a></ul>
          <%end%>
        <%end%>

      <ul>Spouse: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@honey_id%>"><%=@individual.spouse_if_present %></a></ul>

        <%if @individual.sibling !=[] %>
          <%@individual.sibling.each do |sib|%>
            <ul>Sibling: <a href="/family_trees/<%=@tree.id%>/individuals/<%=sib.id%>"><%=sib.name%></a></ul>
          <%end%>
        <%end%>

      <ul>Father: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@dad_id%>"><%=@individual.father_if_present %></a></ul>
      
      <ul>Mother: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@mom_id%>"><%=@individual.mother_if_present %></a></ul>

        <%if @individual.paternal_grandfather_if_present %>
            <ul>Paternal Grandfather: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@grandpad.id%>"><%=@grandpad.name%></a></ul>
        <%end%>

        <%if @individual.paternal_grandmother_if_present %>
            <ul>Paternal Grandmother: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@grandmad.id%>"><%=@grandmad.name%></a></ul>
        <%end%>

          <%if @individual.maternal_grandfather_if_present %>
            <ul>Maternal Grandfather: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@grandpam.id%>"><%=@grandpam.name%></a></ul>
        <%end%>

        <%if @individual.maternal_grandmother_if_present %>
            <ul>Maternal Grandmother: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@grandmam.id%>"><%=@grandmam.name%></a></ul>
        <%end%>
      

      <p><a href="/family_trees/<%=@tree.id%>/individuals/<%=@individual.id%>/edit">Edit Individual Name</a><br>
      <br/><a href="/family_trees/<%=@tree.id%>/individuals/new">Create New Individual</a></p>

    <form action="/family_trees/<%=@tree.id%>/individuals/<%=@individual.id%>/delete" method="post">
      <input id="hidden" type="hidden" name="_method" value="delete"/>
      <input type="submit" name="Delete Individual" id="Delete" value="Delete Individual"/>
    </form>

      <p><a href="/family_trees/<%=@tree.id%>">Back to the Tree</a></p>
      <p><a href="/users/logout">Log Out</a></p>
