<h2><%=warmup_methods%>Welcome <%=current_user.username%>!</h2>
<h3><%=@tree.name%></h3>
<h4><%=@individual.name%>'s Relationships:</h4>
  
  <ul>Father: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@dad_id%>"><%=father_if_present %></a></ul>
  <ul>Mother: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@mom_id%>"><%=mother_if_present %></a></ul>
  <ul>Spouse: <a href="/family_trees/<%=@tree.id%>/individuals/<%=@honey_id%>"><%=spouse_if_present %></a></ul>
  <%if run_gcif? %>
      <%gender_children_if_present.each do |child|%>
  <ul>Child: <a href="/family_trees/<%=@tree.id%>/individuals/<%=child.id%>"><%=child.name%></a></ul>
  <%end%>
  <%end%>
  <%if sibling !=[] %>
      <%sibling.each do |sib|%>
  <ul>Sibling: <a href="/family_trees/<%=@tree.id%>/individuals/<%=sib.id%>"><%=sib.name%></a></ul>
  <%end%>
  <%end%>

<p><a href="/family_trees/<%=@tree.id%>/individuals/<%=@individual.id%>/edit">Edit Individual Name</a>
  <br/><a href="/family_trees/<%=@tree.id%>/individuals/new">Create New Individual</a></p>

<form action="/family_trees/<%=@tree.id%>/individuals/<%=@individual.id%>/delete" method="post">
  <input id="hidden" type="hidden" name="_method" value="delete"/>
  <input type="submit" name="Delete Individual" id="Delete" value="Delete Individual"/>
</form>
<p><a href="/family_trees/<%=@tree.id%>">Back to the Tree</a></p>
<p><a href="/users/logout">Log Out</a></p>
