<h2>Welcome <%=current_user.username%>!</h2>

<h3><%=@tree.name%></h3>
<h4><%=@individual.name%>'s Relationships:</h4>
  
  <ul>Father: <%=father_if_present%></ul>
  <ul>Mother: <%=mother_if_present %></ul>
  <ul>Spouse: <%=spouse_if_present %></ul>
  <%if run_gcif? %>
      <%gender_children_if_present.each do |t|%>
  <ul>Child: <%=t.name%></ul>
  <%end%>
  <%end%>

<p><a href="/family_trees/<%=@tree.id%>/individuals/<%=@individual.id%>/edit">Edit Individual Name</a>
  <br/><a href="/family_trees/<%=@tree.id%>/individuals/new">Create New Individual</a></p>

<form action="/family_trees/<%=@tree.id%>/individuals/<%=@individual.id%>/delete" method="post">
  <input id="hidden" type="hidden" name="_method" value="delete"/>
  <input type="submit" name="Delete Individual" id="Delete" value="Delete Individual"/>
</form>
<p><a href="/family_trees/<%=@tree.id%>">Back to the Tree</a></p>
<p><a href="/users/logout">Log Out</a></p>

